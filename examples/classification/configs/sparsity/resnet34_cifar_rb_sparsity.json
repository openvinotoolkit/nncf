{
  "model": "ResNet34_cifar",
  "input_info": {
    "sample_size": [1, 3, 32, 32]
  },
  "pretrained": false,
  "num_classes": 100,
  "batch_size" : 256,
  "dataset": "CIFAR100",
  "epochs": 300,
  "optimizer": {
    "type": "Adam",
    "base_lr": 1e-4,
    "weight_decay": 0,
    "schedule_type": "plateau",
    "schedule_params": {
      "threshold": 0.1,
      "cooldown": 30
    },
    "parameter_groups": [
      {"lr":  1e-4, "weight_decay":  0, "re": ".*\\._mask"},
      {"lr":  1e-4, "weight_decay":  1e-4}
    ]
  },
  "compression": {
    "algorithm": "rb_sparsity",
    "sparsity_init": 0.3,
    "params": {
      "schedule": "adaptive",
      "patience": 3,
      "sparsity_target": 0.9,
      "sparsity_target_epoch": 5,
      "sparsity_freeze_epoch": 300
    }
  }
}
