{
    "model": "mobilenet_v2",
    "input_info": {
      "sample_size": [2, 3, 32, 32]
    },
    "num_classes": 100,
    "batch_size": 256,
    "optimizer": {
        "type": "Adam",
        "base_lr": 2.73e-5,
        "schedule_type": "plateau",
        "scheduler_params": {
            "cooldown": 3,
            "threshold": 0.1
        },
        "steps": [
            5
        ],
        "weight_decay": 1e-05
    },
    "target_device": "TRIAL",
    "compression": {
        "algorithm": "quantization",
        "activations": {
            "mode": "asymmetric",
            "bits": 4
        },
        "weights": {
            "mode": "asymmetric",
            "bits": 4,
            "per_channel": true
        }
    },
    "epochs": 150,
    "dataset": "CIFAR100"
}