{
    "model": "mobilenet_v2",
    "input_info": {
      "sample_size": [2, 3, 32, 32]
    },
    "num_classes": 100,
    "batch_size": 64,
    "optimizer": {
        "type": "Adam",
        "base_lr": 1e-4,
        "schedule_type": "multistep",
        "steps": [
            30, 40
        ]
    },
    "compression": [
        {
            "algorithm": "quantization",
            "initializer": {
                "range": {
                    "num_init_samples": 256,
                    "type": "min_max"
                }
            }
        },
        {
            "algorithm": "rb_sparsity",
            "sparsity_init": 0.01,
            "params": {
                "sparsity_target": 0.52,
                "sparsity_target_epoch": 10,
                "sparsity_freeze_epoch": 20
            },
            "ignored_scopes": ["MobileNetV2/Sequential[features]/ConvBNReLU[0]/NNCFConv2d[0]"]
        }
    ],
    "epochs": 50,
    "dataset": "CIFAR100"
}