{
    "model": "mobilenet_v2",
    "pretrained": true,
    "input_info": {
        "sample_size": [
            2,
            3,
            224,
            224
        ]
    },
    "num_classes": 1000,
    "batch_size": 200,
    "workers": 6,
    "multiprocessing_distributed": true,
    "optimizer": {
        "base_lr": 3.1e-4,
        "schedule_type": "plateau",
        "type": "Adam",
        "schedule_params": {
            "threshold": 0.1,
            "cooldown": 3
        },
        "weight_decay": 1e-05
    },
    "target_device": "VPU",
    "compression": {
        "algorithm": "quantization",
        "quantize_inputs": true,
        "initializer": {
            "precision": {
                "type": "hawq",
                "bits": [4, 8],
                "compression_ratio": 1.5
            }
        }
    }
}
