{
    "model": "mobilenet_v2",
    "pretrained": true,
    "input_info": {
      "sample_size": [2, 3, 224, 224]
    },
    "num_classes": 1000,
    "batch_size" : 200,
    "epochs": 10,
    "workers": 6,
    "multiprocessing_distributed": true,
    "target_device": "VPU",
    "compression": {
        "algorithm": "quantization",
        "initializer": {
            "batchnorm_adaptation": {
                "num_bn_adaptation_samples": 4000
            },
            "range":
            {
                "type": "mean_min_max",
                "num_init_samples": 1500
            },
            "precision": {
                "type": "autoq",
                "bits": [2, 4, 8],
                "iter_number": 300,
                "compression_ratio": 0.15,
                "eval_subset_ratio": 0.20,
                "dump_init_precision_data": true
            }
        },
        "params": {
            "base_lr": 3.1e-4,
            "base_wd": 1e-5,
            "batch_multiplier": 1,
            "activations_quant_start_epoch": 0,
            "weights_quant_start_epoch": 0,
            "lr_poly_drop_start_epoch": 0,
            "lr_poly_drop_duration_epochs": 10,
        }
    }
}
