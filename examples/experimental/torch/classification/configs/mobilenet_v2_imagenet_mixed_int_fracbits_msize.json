{
    "model": "mobilenet_v2",
    "pretrained": true,
    "input_info": {
      "sample_size": [2, 3, 224, 224]
    },
    "num_classes": 1000,
    "batch_size" : 256,
    "epochs": 5,
    "optimizer": {
        "type": "Adam",
        "base_lr": 0.0001
    },
    "compression": {
        "algorithm": "fracbits_quantization",
        "quantize_inputs": true,
        "freeze_epoch": 4,
        "loss": {
            "type": "model_size",
            "compression_rate": 1.5,
            "criteria": "L1"
        }
    }
}
