{
    "model": "mobilenet_v2",
    "pretrained": true,
    "input_info": {
      "sample_size": [2, 3, 224, 224]
    },
    "num_classes": 1000,
    "batch_size" : 1024,
    "epochs": 5,
    "multiprocessing_distributed": true,
    "optimizer": {
        "type": "Adam",
        "base_lr": 0.00001,
        "schedule_type": "multistep",
        "steps": [
            5
        ]
    },
    "compression": {
        "algorithm": "quantization",
        "overflow_fix": "disable",
        "initializer": {
            "range": {
                "num_init_samples": 2560
            }
        }
    }
}
