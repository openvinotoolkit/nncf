{
    "model": "inception_v3",
    "pretrained": true,
    "input_info": {
      "sample_size": [2, 3, 299, 299]
    },
    "num_classes": 1000,
    "batch_size" : 512,
    "epochs": 1,
    "multiprocessing_distributed" : true,
    "find_unused_parameters": true,
    "optimizer": {
        "type": "SGD",
        "base_lr": 1e-3,
        "schedule_type": "multistep",
        "steps": [
            1
        ]
    },
    "compression": {
        "algorithm": "fracbits_quantization",
        "overflow_fix": "disable",
        "initializer": {
            "range": {
                "num_init_samples": 160
            }
        },
        "ignored_scopes": [
            "Inception3/__add___0",
            "Inception3/__add___1",
            "Inception3/__add___2",
            "Inception3/__mul___0",
            "Inception3/__mul___1",
            "Inception3/__mul___2",
            "Inception3/cat_0"
        ],
        "freeze_epoch": -1,
        "loss": {
            "type": "model_size",
            "compression_rate": 1.5,
            "criteria": "L1",
            "flip_loss": false,
            "alpha": 40.0
        }
    }
}
