{
    "model": "InceptionV3",
    "pretrained": true,
    "input_info": {
        "sample_size": [1, 299, 299, 3]
    },

    "batch_size": 256,
    "epochs": 10,

    "optimizer": {
        "type": "Adam",
        "schedule_type": "piecewise_constant",
        "schedule_params": {
            "boundaries": [5],
            "values": [1e-5, 1e-6]
        }
    },

    "dataset": "imagenet2012",
    "dataset_type": "tfds",

    "compression": {
        "algorithm": "quantization",
        "initializer": {
            "batchnorm_adaptation": {
                "num_bn_adaptation_samples": 2048
            }
        },
        "weights": {
            "per_channel": false
        },
        "activations": {
            "per_channel": false
        }
    }
}
