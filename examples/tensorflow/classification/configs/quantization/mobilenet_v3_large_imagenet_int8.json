{
    "model": "MobileNetV3Large",
    "pretrained": true,
    "input_info": {
        "sample_size": [1, 224, 224, 3]
    },

    "batch_size": 256,
    "epochs": 20,

    "optimizer": {
        "type": "Adam",
        "schedule_type": "piecewise_constant",
        "schedule_params": {
            "boundaries": [5, 10],
            "values": [1e-4, 1e-5, 1e-6]
        }
    },

    "dataset": "imagenet2012",
    "dataset_type": "tfds",

    "compression": {
        "algorithm": "quantization",
        "initializer": {
            "batchnorm_adaptation": {
                "num_bn_adaptation_samples": 2048,
            }
        },
        "weights": {
           "mode": "symmetric",
           "bits": 8,
           "per_channel": true
       },
        "activations": {
            "mode": "asymmetric",
            "bits": 8,
            "per_channel": false
        }
   }
}
