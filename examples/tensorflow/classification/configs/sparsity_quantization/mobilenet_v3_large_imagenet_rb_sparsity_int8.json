{
    "model": "MobileNetV3Large",
    "pretrained": true,
    "input_info": {
        "sample_size": [1, 224, 224, 3]
    },

    "batch_size": 256,
    "epochs": 80,

    "optimizer": {
        "type": "adam",
        "schedule_type": "piecewise_constant",
        "schedule_params": {
            "boundaries": [35, 65],
            "values": [1e-4, 1e-5, 1e-6]
        }
    },

    "dataset": "imagenet2012",
    "dataset_type": "tfds",

    "compression":[
        {
            "algorithm": "rb_sparsity",
            "sparsity_init": 0.01,
            "params": {
                "sparsity_target": 0.42,
                "sparsity_target_epoch": 25,
                "sparsity_freeze_epoch": 45
            }
        },
        {
            "algorithm": "quantization",
            "weights": {
                "mode": "symmetric",
                "bits": 8,
                "per_channel": true
            },
            "activations": {
                "mode": "asymmetric",
                "bits": 8,
                "per_channel": false
            }
        }]
}
