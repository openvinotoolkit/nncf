{
    "model": "MobileNetV2",
    "pretrained": true,
    "input_info": {
        "sample_size": [1, 224, 224, 3]
    },

    "batch_size": 256,
    "epochs": 50,

     "optimizer": {
        "type": "Adam",
        "schedule_type": "multistep",
        "schedule_params": {
            "base_lr": 1e-04,
            "steps": [20],
            "gamma": 0.1
        }
    },

    "dataset": "imagenet2012",
    "dataset_type": "tfds"

}
