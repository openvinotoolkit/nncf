{
    "model": "MobileNetV3Small",
    "pretrained": true,
    "input_info": {
        "sample_size": [1, 224, 224, 3]
    },

    "batch_size": 256,
    "epochs": 90,

    "optimizer": {
        "type": "adam",
        "schedule_type": "piecewise_constant",
        "schedule_params": {
            "boundaries": [60, 85],
            "values": [1e-4, 1e-5, 1e-6]
        }
    },

    "dataset": "imagenet2012",
    "dataset_type": "tfds",

    "compression":[
        {
            "algorithm": "rb_sparsity",
            "sparsity_init": 0.01,
            "params": {
                "schedule": "multistep",
                "multistep_steps": [
                    5,
                    10,
                    15,
                    20,
                    25,
                    30,
                    35,
                    40
                ],
                "multistep_sparsity_levels": [
                    0.1,
                    0.14,
                    0.18,
                    0.22,
                    0.26,
                    0.30,
                    0.34,
                    0.38,
                    0.42
                ],
                "sparsity_freeze_epoch": 50
            }
        },
        {
            "algorithm": "quantization",
            "weights": {
                "mode": "symmetric",
                "bits": 8,
                "per_channel": true
            },
            "activations": {
                "mode": "asymmetric",
                "bits": 8,
                "per_channel": false
            }
        }]
}
