{
    "model": "resnet18",
    "pretrained": true,
    "batch_size" : 512,
    "epochs": 100,
    "input_info": {
      "sample_size": [1, 3, 224, 224]
    },
    "optimizer": {
        "type": "SGD",
        "base_lr": 0.1,
        "weight_decay": 1e-4,
        "schedule_type": "multistep",
        "steps": [
            20,
            40,
            60,
            80
        ],
        "optimizer_params":
        {
            "momentum": 0.9,
            "nesterov": true
        }
    },
   "compression": [
       {
           "algorithm": "filter_pruning",
           "pruning_init": 0.1,
            "params": {
            "schedule": "exponential",
                "pruning_target": 0.4,
                "pruning_steps": 15,
                "filter_importance": "L2"
            }
        }
   ]
}
