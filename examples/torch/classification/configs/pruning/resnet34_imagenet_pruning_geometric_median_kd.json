{
    "model": "resnet34",
    "pretrained": true,
    "batch_size" : 512,
    "epochs": 100,
    "input_info": {
      "sample_size": [1, 3, 224, 224]
    },
    "optimizer": {
        "type": "SGD",
        "base_lr": 0.1,
        "weight_decay": 1e-4,
        "schedule_type": "multistep",
        "steps": [
            25,
            45,
            65,
            80
        ],
        "optimizer_params":
        {
            "momentum": 0.9,
            "nesterov": true
        }
    },
   "compression": [
       {
            "algorithm": "filter_pruning",
            "pruning_init": 0.1,
            "params": {
            "schedule": "exponential",
                "pruning_target": 0.5,
                "pruning_steps": 20,
                "filter_importance": "geometric_median"
            }
       },
       {
            "algorithm": "knowledge_distillation",
            "type": "softmax" // or mse
       }
   ]
}
