{
    "model": "mobilenet_v2",
    "pretrained": true,
    "input_info": {
      "sample_size": [2, 3, 224, 224]
    },
    "num_classes": 1000,
    "batch_size" : 256,
    "epochs": 10,
    "multiprocessing_distributed" : true,
    "optimizer": {
        "type": "Adam",
        "base_lr": 0.00001,
        "schedule_type": "multistep",
        "steps": [
            5
        ]
    },
    "compression": {
        "algorithm": "quantization",
        "initializer": {
            "range": {
                "num_init_samples": 7000
            },
            "batchnorm_adaptation": {
                "num_bn_adaptation_samples": 4096
            }
        },
	    "weights": {
            "per_channel": false
	    },
	    "activations": {
            "per_channel": false
	    }
    }
}
