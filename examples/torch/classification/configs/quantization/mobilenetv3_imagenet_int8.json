{
    "model": "mobilenetv3",
    "input_info": {
      "sample_size": [2, 3, 224, 224]
    },
    "num_classes": 1000,
    "batch_size": 256,
    "weight_decay": 1e-5,
    "multiprocessing_distributed" : true,
    "optimizer": {
        "type": "Adam",
        "base_lr": 1e-4,
        "schedule_type": "plateau",
	"schedule_params": {
     	 "threshold": 0.1,
     	 "cooldown": 30
  	  }
    },
    "compression": {
        "algorithm": "quantization",
        "initializer": {
            "range": {
                "num_init_samples": 2560
            }
        }
    }
}