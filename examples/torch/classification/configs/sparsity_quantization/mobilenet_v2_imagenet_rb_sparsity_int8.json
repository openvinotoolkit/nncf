{
    "model": "mobilenet_v2",
    "pretrained": true,
    "input_info": {
      "sample_size": [1, 3, 224, 224]
    },
    "num_classes": 1000,
    "batch_size" : 256,
    "checkpoint_save_dir": "results/snapshots",
    "epochs": 40,
    "optimizer": {
        "type": "Adam",
        "base_lr": 0.001,
        "schedule_type": "multistep",
        "steps": [20, 30, 40]
    },
    "compression": [
        {
            "algorithm": "rb_sparsity",
            "sparsity_init": 0.01,
            "params": {
                "sparsity_target": 0.52,
                "sparsity_target_epoch": 5,
                "sparsity_freeze_epoch": 10
            },
            "ignored_scopes": ["{re}MobileNetV2/Sequential\\[features\\]/ConvBNActivation\\[0\\]/NNCFConv2d\\[0\\]/.*"]
        },
        {
            "algorithm": "quantization",
            "initializer": {
                "range": {
                    "num_init_samples": 256,
                    "type": "min_max"
                }
            }
        }
    ]
}
