{
    "model": "resnet50",
    "pretrained": true,
    "input_info": {
        "sample_size": [1, 3, 224, 224]
    },
    "num_classes": 1000,
    "batch_size": 640, // 64 per GPU
    "epochs": 23,
    "multiprocessing_distributed": true,
    "optimizer": {
        "type": "Adam",
        "base_lr": 0.001,
        "schedule_type": "multistep",
        "steps": [15, 20, 25]
    },
    "compression": [
        {
            "algorithm": "rb_sparsity",
            "sparsity_init": 0.01,
            "params": {
                "sparsity_target": 0.5,
                "sparsity_target_epoch": 5,
                "sparsity_freeze_epoch": 10
            }
        },
        {
            "algorithm": "quantization"
        }
    ]
}
