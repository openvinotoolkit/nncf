{
    "model": "resnet50",
    "pretrained": true,
    "input_info": {
        "sample_size": [
            1,
            3,
            224,
            224
        ]
    },
    "num_classes": 1000,
    "batch_size" : 150,
    "epochs": 8,
    "workers": 6,
    "multiprocessing_distributed": true,
    "target_device": "NPU",
    "compression": {
        "algorithm": "quantization",
        "initializer": {
            "batchnorm_adaptation": {
                "num_bn_adaptation_samples": 3500
            },
            "range":
            {
                "type": "mean_min_max",
                "num_init_samples": 1000
            },
            "precision": {
                "type": "autoq",
                "bits": [2, 4, 8],
                "iter_number": 300,
                "compression_ratio": 0.15,
                "eval_subset_ratio": 0.20,
                "dump_init_precision_data": true
            }
        },
        "params": {
            "base_lr": 2e-4,
            "base_wd": 1e-5,
            "batch_multiplier": 1,
            "activations_quant_start_epoch": 0,
            "weights_quant_start_epoch": 0,
            "lr_poly_drop_start_epoch": 0,
            "lr_poly_drop_duration_epochs": 10
        }
    },
    "no_strip_on_export": true,
    "export_to_ir_via_onnx": true
}
