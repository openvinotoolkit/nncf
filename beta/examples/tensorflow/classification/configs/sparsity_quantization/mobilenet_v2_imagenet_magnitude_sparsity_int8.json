{
    "model": "MobileNetV2",
    "pretrained": true,
    "input_info": {
        "sample_size": [1, 224, 224, 3]
    },

    "batch_size": 256,
    "epochs": 40,

     "optimizer": {
        "type": "Adam",
        "schedule_type": "piecewise_constant",
        "schedule_params": {
            "boundaries": [20, 28],
            "values": [1e-4, 1e-5, 1e-6]
        }
    },

    "dataset": "imagenet2012",
    "dataset_type": "tfds",

    "compression": [
        {
            "algorithm": "magnitude_sparsity",
            "params": {
                "schedule": "multistep",
                "multistep_steps": [
                    5,
                    10,
                    15,
                    20,
                    21
                ],
                "multistep_sparsity_levels": [
                    0.1,
                    0.15,
                    0.20,
                    0.25,
                    0.30,
                    0.35
                ]
            }
        },
        {
            "algorithm": "quantization"
        }
    ]
}
