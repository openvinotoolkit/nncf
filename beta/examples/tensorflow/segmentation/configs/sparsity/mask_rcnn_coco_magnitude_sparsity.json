{
    "model": "MaskRCNN",
    "input_info": {
        "sample_size": [1, 1024, 1024, 3]
    },

    "epochs": 2,
    "batch_size": 16,
    "weight_decay": 0.0001,

    "dataset": "coco/2017",

     "optimizer": {
        "type": "adam",
        "schedule_type": "piecewise_constant",
        "schedule_params": {
            "boundaries": [1],
            "values": [1e-5, 1e-6]
        }
    },

  "compression": {
        "algorithm": "magnitude_sparsity",
        "params": {
            "schedule": "multistep",
            "multistep_steps": [1],
            "multistep_sparsity_levels": [0.5, 0.5]
        }
    }
}
