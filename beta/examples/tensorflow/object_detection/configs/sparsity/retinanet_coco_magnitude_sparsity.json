{
    "model": "RetinaNet",
    "input_info": {
        "sample_size": [1, 640, 640, 3]
    },

    "epochs": 1,
    "batch_size": 64,
    "weight_decay": 0.0001,

    "dataset": "coco/2017",
    "dataset_type": "tfds",

    "optimizer": {
        "type": "Adam",
        "schedule_type": "piecewise_constant",
        "schedule_params": {
            "boundaries": [
                1
            ],
            "values": [
                1e-5,
                1e-5
            ]
        }
    },

    "compression": {
        "algorithm": "magnitude_sparsity",
        "params": {
            "schedule": "multistep",
            "multistep_steps": [
                1
            ],
            "multistep_sparsity_levels": [
                0.5,
                0.5
            ]
        }
    }
}
