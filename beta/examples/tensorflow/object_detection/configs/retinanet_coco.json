{
    "model": "RetinaNet",
    "input_info": {
        "sample_size": [1, 640, 640, 3]
    },

    "epochs": 12,
    "batch_size": 64,
    "weight_decay": 0.0001,

    "dataset": "coco/2017",
    "dataset_type": "tfds",

    "optimizer": {
        "type": "momentum",
        "optimizer_params": {
            "momentum": 0.9,
            "nesterov": true
        },
        "schedule_type": "step_warmup",
        "schedule_params": {
            "warmup_learning_rate": 0.0067,
            "warmup_steps": 500,
            "init_learning_rate": 0.08,
            "learning_rate_levels": [
                0.008,
                0.0008
            ],
            "learning_rate_steps": [
                15000,
                20000
            ]
        }
    }
}
