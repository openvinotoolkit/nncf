{
    "model": "YOLOv4",
    "input_info": {
        "sample_size": [
            1,
            608,
            608,
            3
        ]
    },

    "epochs": 8,
    "batch_size": 63,
    "workers": 16,

    "dataset": "coco/2017",
    "dataset_type": "tfds",

    "optimizer": {
        "type": "momentum",
        "optimizer_params": {
            "momentum": 0.95
        },

        "schedule_type": "cosine",
        "schedule_params": {
            "initial_lr": 1e-4
        }

    },

    "compression": {
        "algorithm": "quantization",
        "weights": {
            "per_channel": true
        }
    }

}
