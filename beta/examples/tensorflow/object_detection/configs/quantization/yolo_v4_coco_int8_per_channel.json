{
    "model": "YOLOv4",
    "input_info": {
        "sample_size": [
            1,
            608,
            608,
            3
        ]
    },

    "epochs": 15,
    "batch_size": 63,
    "workers": 16,

    "dataset": "coco/2017",
    "dataset_type": "tfds",

    "optimizer": {
        "type": "momentum",
        "optimizer_params": {
            "momentum": 0.95
        },

        "schedule_type": "step",
        "schedule_params": {
            "warmup_learning_rate": 1e-8,
            "warmup_steps": 1000,
            "init_learning_rate": 1e-5,
            "learning_rate_levels": [
                1e-5
            ],
            "learning_rate_steps": [
                50000
            ]
        }
    },

    "compression": {
        "algorithm": "quantization",
        "weights": {
            "per_channel": true
        }
    }

}
