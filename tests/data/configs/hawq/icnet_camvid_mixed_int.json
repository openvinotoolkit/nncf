{
    "model": "icnet",
    "dataset": "camvid",
    "pretrained": false,
    "preprocessing": {
        "resize": {
            "height": 96,
            "width": 96
        },
        "normalize": {
            "mean": [
                0.39068785,
                0.40521392,
                0.41434407
            ],
            "std": [
                0.29652068,
                0.30514979,
                0.30080369
            ]
        }
    },
    "augmentations": {
        "random_hflip": 0.5
    },
    "input_info": {
        "sample_size": [
            1,
            3,
            96,
            96
        ]
    },
    "weighing": "mfb",
    "model_params": {
        "in_channels": 3,
        "input_size_hw": [
            96,
            96
        ]
    },
    "target_device": "VPU",
    "compression": {
        "algorithm": "quantization",
        "initializer": {
            "precision": {
                "type": "hawq",
                "num_data_points": 2,
                "iter_number": 1
            },
            "batchnorm_adaptation": {
                "num_bn_adaptation_samples": 2,
                "num_bn_forget_samples": 1
            }
        }
    }
}
